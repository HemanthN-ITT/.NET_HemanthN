using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

/* 
 * Write a program to Print Inverted Pyramid of Numbers Pattern.

OUTPUT:

1 2 3 4 5

1 2 3 4

1 2 3

1 2

1
 */
namespace ProblemStatement2 {
    internal class Program {
        static void Main(string[] args) {
            int row = 5, col = 5;
            for(int i=0;i<row; i++) {
                for(int j=0;j<col-i;j++) {
                    Console.Write(j + 1 + " ");
                }
                Console.WriteLine();
            }
        }
    }
}
